{
  "name": "mongrel",
  "version": "0.0.1-alpha1",
  "description": "Studio3t Cross platform alternative",
  "main": "public/electron.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pareshchouhan/mongrel.git"
  },
  "author": {
    "name": "Paresh Chouhan",
    "email": "pareshchouhan2013@gmail.com",
    "url": "https://futuretraxex.com"
  },
  "keywords": [
    "electron",
    "mongo",
    "studio-3t",
    "react",
    "mongo-admin"
  ],
  "build": {
    "productName": "Mongrel",
    "appId": "mongrel.futuretraxex.com",
    "win": {
      "icon": "build/icon.ico",
      "target": "portable"
    }
  },
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "antd": "^4.17.2",
    "axios": "^0.24.0",
    "chart.js": "^2.9.4",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.4.1",
    "immer": "^9.0.7",
    "jssoup": "0.0.11",
    "lodash": "^4.17.21",
    "mongodb": "^3.7.3",
    "mongoose": "^5.13.13",
    "react": "^16.14.0",
    "react-chartjs-2": "^2.11.2",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.6",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.0",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "browser-react-start": "node public/browser.js&&set REACT_APP_LITE_MODE=1&&react-scripts start",
    "react-start": "set BROWSER=none&&set REACT_APP_LITE_MODE=0&&react-scripts start",
    "react-watch": "set BROWSER=none&&set REACT_APP_LITE_MODE=0&&react-scripts watch",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-start": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "electron-build": "electron-builder",
    "build:electron": "./node_modules/.bin/tsc --project tsconfig.electron.json",
    "build": "npm run react-build && npm run electron-build",
    "lite-mode": "npm run browser-react-start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/react-redux": "^7.1.20",
    "electron": "^16.0.6",
    "electron-builder": "^22.14.5",
    "node-sass": "^6.0.1",
    "npm-watch": "^0.11.0",
    "redux-devtools-extension": "^2.13.9"
  }
}
